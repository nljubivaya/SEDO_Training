<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SEDO_Training.Test2">
	<Border BorderThickness="2" BorderBrush="#007ACC" Padding="20" CornerRadius="10" Margin="20" Background="#F7F7F7">
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Height="Auto"/>
				<RowDefinition Height="*"/>
			</Grid.RowDefinitions>
			<StackPanel Orientation="Horizontal" Grid.Row="0" HorizontalAlignment="Stretch">
				<!--<Button Command="{Binding ToMain}" Content="Назад" Margin="10" HorizontalAlignment="Right" Background="#007ACC"-->
						Foreground="White" FontWeight="Bold"/>
				<!--<Button  Command="{Binding }" Content="Вернуться к курсу" Margin="10" HorizontalAlignment="Left" Background="#007ACC"-->
						Foreground="White" FontWeight="Bold"/>
			</StackPanel>

			<ScrollViewer Grid.Row="1">
				<StackPanel>
					<TextBlock Text="Тест по теме «»" FontSize="24" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,10"/>

				</StackPanel>
			</ScrollViewer>
		</Grid>
	</Border>
</UserControl>
